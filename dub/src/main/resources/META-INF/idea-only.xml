<idea-plugin>

    <extensions defaultExtensionNs="com.intellij">
        <moduleBuilder id="DLangDubModuleBuilder"
                       builderClass="io.github.intellij.dub.module.DlangDubModuleBuilder"/>

        <projectImportProvider implementation="io.github.intellij.dub.project.DubProjectImportProvider"/>
        <projectImportBuilder implementation="io.github.intellij.dub.project.DubProjectImportBuilder"/>
        <projectOpenProcessor id="DubProjectOpenProcessor" implementation="io.github.intellij.dub.project.DubProjectOpenProcessor"/>
    </extensions>
</idea-plugin>

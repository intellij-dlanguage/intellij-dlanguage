<idea-plugin xmlns:xi="http://www.w3.org/2001/XInclude">
    <xi:include href="/META-INF/dub-idea-only.xml" xpointer="xpointer(/idea-plugin/*)"/>

    <extensions defaultExtensionNs="com.intellij">
        <iconProvider id="dlangIconProvider" implementation="io.github.intellij.dlanguage.DlangIconProvider" />

        <moduleBuilder id="DLangModuleBuilder"
                       builderClass="io.github.intellij.dlanguage.module.DlangModuleBuilder"/>

        <!-- Project Tree View -->
        <treeStructureProvider implementation="io.github.intellij.dlanguage.projectview.DTreeStructureProvider"/>

        <moduleConfigurationEditorProvider
            implementation="io.github.intellij.dlanguage.module.DlangModuleEditorsProvider"/>

    </extensions>

    <extensions defaultExtensionNs="io.github.intellij.dlanguage">
        <projectWizard.dlang.buildSystem implementation="io.github.intellij.dlanguage.project.wizard.generators.IntellijDlangNewProjectWizard" />
    </extensions>

</idea-plugin>

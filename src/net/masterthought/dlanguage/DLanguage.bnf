     ;

 functionLiteralExpression ::=
     (('function' | 'delegate') type?)? (parameters functionAttribute*)? functionBody

 gotoStatement ::=
     'goto' (Identifier | 'default' | 'case' expression?) ';'

 identifierChain ::=

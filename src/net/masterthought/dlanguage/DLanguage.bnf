 staticAssertStatement ::=
     'static' assertExpression ';'


 staticConstructor ::=
     'static' 'this' '(' ')' functionBody


 staticDestructor ::=
     'static' '~' 'this' '(' ')' functionBody


 staticIfCondition ::=
     'static' 'if' '(' assignExpression ')'


 storageClass ::=
       atAttribute
     | typeConstructor
     | deprecated
     | 'abstract'

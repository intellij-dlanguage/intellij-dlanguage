 versionSpecification ::=
//     'version' '=' (Identifier | IntegerLiteral) ';'
     'version' '=' Identifier ';'


 whileStatement ::=
     'while' '(' expression ')' declarationOrStatement


 withStatement ::=
     'with' '(' expression ')' statementNoCaseNoDefault


